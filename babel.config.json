{
  "env": {
    "es5": {
      "presets": ["@babel/preset-env"],
      "plugins": [
        [
          "@babel/plugin-transform-runtime",
          {
            "corejs": 3
          }
        ]
      ]
    },
    "es6": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": "supports es6-module"
          }
        ]
      ],
      "plugins": [
        [
          "@babel/plugin-transform-runtime",
          {
            "corejs": 3
          }
        ]
      ]
    }
  }
}
